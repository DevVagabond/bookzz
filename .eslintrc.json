{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "mocha": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "consistent-return": 0,
        "import/order": [
            "error",
            {
                "groups": [
                    "internal",
                    "external",
                    "builtin",
                    "object"
                ]
            }
        ]
    }
}